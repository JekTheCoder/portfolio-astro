{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/public/*": [
        "./public/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
			"@/content/*": [
				"./src/content/*"
			]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
